{
    "name": "mikron-ia/rpg-hub",
    "description": "Integrated RPG hub",
    "keywords": ["yii2", "rpg"],
    "type": "project",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
		"yidas/yii2-bower-asset": "~2.0.5",
        "yiisoft/yii2": "^2.0",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "kartik-v/yii2-krajee-base": "^3.0",
        "kartik-v/yii2-widget-select2": "^2.0",
        "kartik-v/yii2-password": "^1.5",
        "kartik-v/strength-meter": "^1.1",
        "yii2tech/ar-position": "^1.0",
        "vlucas/phpdotenv": "^5.5",
        "kartik-v/yii2-widget-datepicker": "^1.4"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "phpunit/phpunit": "^10.3"
    },
    "config": {
        "process-timeout": 1800,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload-dev": {
        "classmap": [
            "common/models/core/",
            "common/models/tools/"
        ]
    }
}
